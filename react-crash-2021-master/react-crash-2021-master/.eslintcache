 => {\n    const res = await fetch(`http://localhost:5000/characters/${id}`)\n    const data = await res.json()\n\n    return data\n  }\n\n  // Add Character\n  const addCharacter = async (character) => {\n    const res = await fetch('http://localhost:5000/characters', {\n      method: 'POST',\n      headers: {\n        'Content-Type': 'application/json',\n      },\n     \n      body: JSON.stringify(character),\n    })\n    console.log(JSON.stringify(character));\n    const data = await res.json()\n\n    setCharacters([...characters, data])\n  }\n\n  // Delete Character\n  const deleteCharacter = async (id) => {\n    const res = await fetch(`http://localhost:5000/characters/${id}`, {\n      method: 'DELETE',\n    })\n    //We should control the response status to decide if we will change the state or not.\n    res.status === 200\n      ? setCharacters(characters.filter((character) => character.id !== id))\n      : alert('Error Deleting This Character')\n  }\n\n    \n\n  return (\n    <Router>\n      <div className='container'>\n        <Header\n          onAdd={() => setShowAddCharacter(!showAddCharacter)}\n          showAdd={showAddCharacter}\n        />\n        <Routes>\n          <Route\n            path='/'\n            element={\n              <>\n                {showAddCharacter && <AddCharacter onAdd={addCharacter} />}\n                {characters.length > 0 ? (\n                  <Characters\n                    characters={characters}\n                    onDelete={deleteCharacter}\n                  />\n                ) : (\n                  'No Characters To Show'\n                )}\n              </>\n            }\n          />\n          <Route path='/about' element={<About />} />\n        </Routes>\n        <Footer />\n      </div>\n    </Router>\n  )\n}\n\nexport default App\n","C:\\Users\\Sebek\\Downloads\\react-crash-2021-master\\react-crash-2021-master\\src\\components\\About.js",[],"C:\\Users\\Sebek\\Downloads\\react-crash-2021-master\\react